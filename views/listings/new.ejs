<html>
  <head>
    <title>home_Me</title>
    <script src="https://code.jquery.com/jquery-3.1.1.min.js" integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8=" crossorigin="anonymous"></script>
    <link rel='stylesheet' href='/css/newlisting.css'/>
    <link rel='stylesheet' href='/css/navbar.css'/>
    <link rel="stylesheet" href="/assets/css/main.css" />
  </head>
  <body>
    <!-- Wrapper -->
    <div id="wrapper">
      <!-- Header -->
      <%- include('../partials/navbar.ejs')  %>
      <!-- Main -->
      <div id="main">
        <div class="inner">
          <section>
            <div class="row">
              <div class="6u$ 12u$(medium)"></div>
            </div>
            <section>
              <form method="post" action="#">
                <div class="row uniform">
                    <div class="6u 12u$(xsmall)">
                        <input type="text" name="demo-name" id="city" value="" placeholder="city" />
                    </div>
                    <div class="6u$ 12u$(xsmall)">
                        <input type="text" name="demo-name" id="state" value="" placeholder="state" />
                    </div>
                    <div class="6u 12u$(xsmall)">
                        <input type="text" name="demo-name" id="address" value="" placeholder="address" />
                    </div>
                    <div class="6u$ 12u$(xsmall)">
                        <input type="text" name="demo-name" id="zipcode" value="" placeholder="zipcode" />
                    </div>
                    <div class="6u 12u$(xsmall)">
                        <input type="text" name="demo-name" id="price" value="" placeholder="price" />
                    </div>
                    <div class="6u$ 12u$(xsmall)">
                        <input type="text" name="demo-name" id="bedrooms" value="" placeholder="bedrooms" />
                    </div>
                    <div class="6u 12u$(xsmall)">
                        <input type="email" name="demo-email" id="bathrooms" value="" placeholder="bathrooms" />
                    </div>
                    <div class="6u$ 12u$(xsmall)">
                        <input type="text" name="demo-name" id="duration" value="" placeholder="duration"/>
                    </div>
                    <div class="6u 12u$(xsmall)">
                        <input type="text" name="demo-name" id="pets" value="" placeholder="pets"/>
                    </div>
                    <div class="6u$ 12u$(xsmall)">
                        <input type="email" name="demo-email" id="furnished" value="" placeholder="furnished"/>
                    </div>
                </div>
              </form>
            </section>
            <div class="row">
              <div class="6u 12u$(medium)">
                <input type="hidden" id="imageurl" name="imageurl" value="/assets/images/default.png">
                <input type="file" name="file" id="file-input" class="inputfile" />
                <label class="button icon fa-upload" for="file-input">Choose a file</label>
                <br>
                <img style='width:100px'id="preview" src="/assets/images/default.png">
                <br>
                <br>
                <ul class="actions">
                  <li><a href="#" class="button special" id="submitListing">Submit</a></li>
                </ul>
              </div>
            </div>
          </section>
        </div>
      </div>
        <!-- Footer -->
        <footer id="footer">
          <div class="inner">
            <section>
              <ul class="icons">
                <li><a href="#" class="icon style2 fa-twitter"><span class="label">Twitter</span></a></li>
                <li><a href="#" class="icon style2 fa-facebook"><span class="label">Facebook</span></a></li>
                <li><a href="#" class="icon style2 fa-instagram"><span class="label">Instagram</span></a></li>
                <li><a href="#" class="icon style2 fa-github"><span class="label">GitHub</span></a></li>
                <li><a href="#" class="icon style2 fa-envelope-o"><span class="label">Email</span></a></li>
              </ul>
            </section>
            <ul class="copyright">
              <li>&copy; Untitled. All rights reserved</li>
              <li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
            </ul>
          </div>
        </footer>
    </div>
    <!-- Scripts -->
    <script rel='javascript' src='/js/listings.js'></script>
    <script rel='javascript' src='/js/newlisting.js'></script>
    <script src="/assets/js/jquery.min.js"></script>
    <script src="/assets/js/skel.min.js"></script>
    <script src="/assets/js/util.js"></script>
    <script src="/assets/js/main.js"></script>
  </body>
</html>
